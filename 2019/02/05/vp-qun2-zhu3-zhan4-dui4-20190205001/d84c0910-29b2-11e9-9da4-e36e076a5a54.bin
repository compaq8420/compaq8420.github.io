<script language="javascript" type="text/javascript">
	function getParameterByName(name, url) {
		if (!url) {
			url = window.location.href;
		}
		name = name.replace(/[\[\]]/g, "\\$&");
		var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"), results = regex.exec(url);
		if (!results) return null;
		if (!results[2]) return '';
		return decodeURIComponent(results[2].replace(/\+/g, " "));
	}

	function initPlaylist(type) {
		if(type == 'share') $('#djList .titShare').addClass('border'); else $('#djList .titShare').removeClass('border');
		if(type == 'hot') $('#djList .titHot').addClass('border'); else $('#djList .titHot').removeClass('border');
		if(type == 'new') $('#djList .titNew').addClass('border'); else $('#djList .titNew').removeClass('border');
		//$('#djList .titHot').removeClass('border');
		//$('#djList .titNew').removeClass('border');
		if(type == 'share') $(".plShare").show(); else $(".plShare").hide();
		if(type == 'hot') $(".plHot").show(); else $(".plHot").hide();
		if(type == 'new') $(".plNew").show(); else $(".plNew").hide();
		//$(".plHot").hide();
		//$(".plNew").hide();
	}
	var type = getParameterByName('playsrc');
	if(type == null) type = 'hot';
	initPlaylist(type);
	$(".tit").on("click",function(){
		$(this).siblings().removeClass("border");
		$(this).addClass("border");
		var num=$(this).index();
		$(".playList").eq(num).show();
		$(".playList").eq(num).siblings().hide();
	});
</script>
